{
  "name": "people.profile.socialpage.types",
  "description": "Social Page objects are user defined page or profile entries that reference other platforms",
  "api_endpoints": [
    {
      "endpoint": "/people/profile/socialpage-types",
      "description": "GET: List of People Profile Social Page Type options using optional 'search' parameter"
    },
    {
      "endpoint": "/people/{uuid}/profile",
      "description": "PATCH: overwrite People Profile Social Page Entry when found in request body"
    }
  ],
  "key_value_pairs": {
    "bitbucket": "Bitbucket Social Page - https://bitbucket.org",
    "facebook": "Facebook Social Page - https://www.facebook.com",
    "github": "GitHub Social Page - https://github.com",
    "gitlab": "GitLab Social Page - https://gitlab.com",
    "google": "Google Social Page - https://www.google.com",
    "instagram": "Instagram Social Page - https://www.instagram.com",
    "linkedin": "LinkedIn Social Page - https://www.linkedin.com",
    "messenger": "Messenger Social Page - https://www.facebook.com/messenger",
    "other": "Other Social Page - generic",
    "pinterest": "Pinterest Social Page - https://www.pinterest.com",
    "twitter": "Twitter Social Page - https://twitter.com",
    "youtube": "YouTube Social Page - https://www.youtube.com"
  }
}